#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    int s=0;
    int sq_sum=0;
    for( int i=1; i<=100; i++ )
    {
        s+=i;
        sq_sum+=i*i;
    }
    s*=s;
    //cout << "Suma kwadratow: " << sq_sum << endl;
    //cout << "Kwadrat sumy: " << s << endl;
    int diff=s-sq_sum;
    cout << "Roznica: " << diff << endl;
    return 0;
}
